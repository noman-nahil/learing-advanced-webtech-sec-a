<!DOCTYPE html>
<html>
<head>
	<title>Update Employee Information</title>
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Create New User</legend>
			<table>
				<tr>
					<td>Company Name</td>
					<td><input type="text" name="cname" value="<%= cname%>"></td>
				</tr>
				<tr>
					<td>Job title</td>
					<td><input type="text" name="jobtitle" value="<%= jobtitle%>"></td>
				</tr>
				<tr>
					<td>Location</td>
					<td><input type="text" name="location" value="<%= location%>"></td>
				</tr>
				<tr>
					<td>Salary</td>
					<td><input type="text" name="salary" value="<%= salary %>"></td>
				</tr>
					<td></td>
					<td><input type="submit" name="submit" value="Job Update"></td>
				</tr>
			</table>
		</fieldset>
	</form>
</body>
</html>